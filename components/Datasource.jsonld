{
  "@context": "https://linkedsoftwaredependencies.org/contexts/ldf-server.jsonld",
  "@id": "npmd:ldf-server",
  "components": [
    {
      "@id": "ldfs:Datasource",
      "@type": "AbstractClass",
      "parameters": [
        {
          "@id": "rdfs:label",
          "defaultScoped": {
            "defaultScope": "ldfs:Datasource",
            "defaultScopedValue": "My datasource"
          }
        },
        {
          "@id": "ldfs:Datasource/description",
          "comment": "The datasource description",
          "range": "xsd:string",
          "unique": true
        },
        {
          "@id": "ldfs:Datasource/path",
          "comment": "The relative path to the datasource from the baseURL",
          "range": "xsd:string",
          "unique": true
        },
        {
          "@id": "ldfs:Datasource/enabled",
          "comment": "If the datasource is enabled, by default true",
          "range": "xsd:boolean",
          "unique": true
        },
        {
          "@id": "ldfs:Datasource/hidden",
          "comment": "If the datasource must be hidden from the index, by default false",
          "range": "xsd:boolean",
          "unique": true
        },
        {
          "@id": "ldfs:Datasource/graph",
          "comment": "The default graph of the datasource",
          "range": "xsd:string",
          "unique": true
        },
        {
          "@id": "ldfs:Datasource/urlData",
          "inheritValues": {
            "@type": "InheritanceValue",
            "onParameter": "ldfs:urlData",
            "from": "ldfs:Server/Qpf"
          }
        },
        {
          "@id": "ldfs:Datasource/license",
          "comment": "The license of the datasource",
          "range": "xsd:string",
          "unique": true
        },
        {
          "@id": "ldfs:Datasource/licenseUrl",
          "comment": "A link to the license of the datasource",
          "unique": true
        },
        {
          "@id": "ldfs:Datasource/copyright",
          "comment": "The copyright statement of the datasource",
          "range": "xsd:string",
          "unique": true
        },
        {
          "@id": "ldfs:Datasource/homepage",
          "comment": "The homepage url of the datasource",
          "unique": true
        }
      ],
      "constructorArguments": [
        {
          "@id": "ldfs:Datasource/constructorArgumentsObject",
          "fields": [
            {
              "keyRaw": "id",
              "value": "rdf:subject"
            },
            {
              "keyRaw": "title",
              "value": "rdfs:label"
            },
            {
              "keyRaw": "description",
              "value": "ldfs:Datasource/description"
            },
            {
              "keyRaw": "path",
              "value": "ldfs:Datasource/path"
            },
            {
              "keyRaw": "enabled",
              "value": "ldfs:Datasource/enabled"
            },
            {
              "keyRaw": "hide",
              "value": "ldfs:Datasource/hidden"
            },
            {
              "keyRaw": "graph",
              "value": "ldfs:Datasource/graph"
            },
            {
              "@id": "ldfs:Server/Qpf/ObjectMappings/urlData"
            },
            {
              "keyRaw": "license",
              "value": "ldfs:Datasource/license"
            },
            {
              "keyRaw": "licenseUrl",
              "value": "ldfs:Datasource/licenseUrl"
            },
            {
              "keyRaw": "copyright",
              "value": "ldfs:Datasource/copyright"
            },
            {
              "keyRaw": "homepage",
              "value": "ldfs:Datasource/homepage"
            }
          ]
        }
      ]
    }
  ]
}
